{
	"env": {
		"browser": true,
		"es6": true,
		"mocha": true,
		"node": true
	},
    "extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"parser": "babel-eslint",
	"rules": {
		"react/jsx-uses-react": "error",
		"react/jsx-uses-vars": "error",
	}
}